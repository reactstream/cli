{
  "name": "@reactstream/cli",
  "version": "0.3.1",
  "description": "CLI tool for React component development and debugging",
  "main": "index.js",
  "bin": {
    "reactstream": "index.js",
    "reactstream-analyze": "bin/reactstream-analyze.js"
  },
  "scripts": {
    "start": "node index.js",
    "test": "echo \"Warning: No test specified, skipping tests\" && exit 0",
    "lint": "eslint src/**/*.js commands/**/*.js || true",
    "prepare": "npm run lint",
    "version": "bash version.sh",
    "publish": "bash publish.sh",
    "upgrade": "bash upgrade.sh",
    "analyze": "node index.js analyze",
    "serve": "node index.js serve",
    "clean": "bash start.sh clean",
    "build": "echo \"No build step required\"",
    "release:patch": "npm version patch",
    "release:minor": "npm version minor",
    "release:major": "npm version major",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "babel-loader": "^8.2.5",
    "chalk": "^4.1.2",
    "css-loader": "^5.2.7",
    "escodegen": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "esprima": "^4.0.1",
    "estraverse": "^5.2.0",
    "express": "^4.17.1",
    "minimist": "^1.2.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "style-loader": "^2.0.0",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "react",
    "cli",
    "development",
    "analysis",
    "debugging",
    "components",
    "optimization"
  ],
  "author": "ReactStream Team",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reactstream/cli.git"
  },
  "bugs": {
    "url": "https://github.com/reactstream/nodejs/issues"
  },
  "homepage": "https://github.com/reactstream/nodejs#readme"
}
